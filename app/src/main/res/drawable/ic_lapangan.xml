<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="60dp"
    android:viewportWidth="120"
    android:viewportHeight="60">

    <!-- Badan lapangan -->
    <path
        android:fillColor="#BBBBBB"
        android:pathData="M10,10 L110,10 L110,50 L10,50 Z" />

    <!-- Garis tengah vertikal dan horizontal -->
    <!-- PERBAIKAN: pathData untuk stroke tidak boleh berisi fillColor -->
    <path
        android:strokeColor="#000000"
        android:strokeWidth="1"
        android:pathData="M60,10 L60,50 M10,30 L110,30" />

    <!-- Lingkaran tengah -->
    <!-- PERBAIKAN: Hapus namespace 'android:' dari cx, cy, r, dan fillColor -->
    <circle
        cx="60"
        cy="30"
        r="8"
        fillColor="#000000" />

</vector>
